#include <stdio.h> 


int facto(int); // function prototype 原型宣告 
double expo(int);
double power(int, int);
int main(void)
{
	int x;
	// loop 10 times and calculate and output square of x each time
	for (x = 1; x <= 15; ++x) {
		printf("%d  ", facto(x)); // function call
	}
	printf("\n%lf ", expo(10));
	puts("");


	double a;
	double b;
	printf("\n%lf", power(2, 3));
}

// square function definition returns the square of its parameter 
int facto(int y) // y is a copy of the argument to the function
{
	int result = 1, i;
	for (i = 1; i <= y; i++)
		result = result * i;
	return result; // returns the square of y as an int              
}

double expo(int y) {
	int result = 1, i;
	double expo = 1;
	for (i = 1; i <= y; i++) {
		result = result * i;
		expo = expo + (1.0 / result);
	}
	return expo;
}

double power(int a, int b) {
	
	double power = a^b;
	return power;
}
