#include <stdio.h>

int fac(int);

int main()
{
    int n;
    scanf_s("%d", &n);
    for (int i = 0; i <= n; i++) {
        printf("%d", fac(i));
    }
    printf("\n");
    printf("%d\n", fac(n));

   //迭代方式

    printf("Q2\n");
    scanf_s("%d", &n);
    int n1 = 0, n2 = 1, asw;

    if (n <= 1)printf("%d", n);

    else
    {
        for (int i = 2; i <= n; i++) {
            asw = n1 + n2;
            n1 = n2;
            n2 = asw;
        }
        printf("%d", asw);
    }

}

//遞迴方式
int fac(int n) {
    if (n <= 1)return n;

    else return fac(n - 1) + fac(n - 2);
}
